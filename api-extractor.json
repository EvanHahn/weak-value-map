{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/build/index.d.ts",
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/build/<unscopedPackageName>.api.json"
  },
  "dtsRollup": {
    "enabled": false
  },
  "newlineKind": "lf",
  "tsdocMetadata": {
    "enabled": false
  }
}
